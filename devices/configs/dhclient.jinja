# Generated by devices/entrypoint.sh
timeout 30;
retry 60;
reboot 5;
script "/sbin/dhclient-script";

interface "eth0" {
  send host-name "{{ hostname }}";
  send vendor-class-identifier "{{ vendor_class }}";
  send dhcp-client-identifier "{{ client_id }}";
  request subnet-mask, routers, domain-name, domain-name-servers, host-name, interface-mtu;
  require subnet-mask, domain-name-servers;
  timeout 30;
  retry 60;
}
